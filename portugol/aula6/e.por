	  	Enunciado: e) Um número não zero ?
    */
	funcao inicio() {
		//variaveis
		inteiro n1
		logico cal
		
		//dados
		escreva("Digite um numero!: ")
		leia(n1)
		
		//computação
		cal = (n1 != 0)
		
		//resultado
		escreva("Um número não zero? " , cal)
		
		
	}
}
